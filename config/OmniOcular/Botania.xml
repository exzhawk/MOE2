<oo>
    <tileentity id="botania:pool">
        <line displayname="hud.msg.Botania.Mana">
            return nbt['mana']+GRAY+" / "+WHITE+nbt['manaCap']
        </line>
		<line>
            switch(nbt['outputting']) {
				case 0:return translate('hud.msg.Botania.output.0');
					break;
				case 1:return translate('hud.msg.Botania.output.1');
					break;
				}
        </line>
    </tileentity>
    <tileentity id="botania:spreader">
        <line displayname="hud.msg.Botania.Mana">
            return nbt['mana']
        </line>
    </tileentity>
    <tileentity id="botania:runeAltar">
        <line displayname="hud.msg.Botania.Mana">
            return nbt['mana']+GRAY+" / "+WHITE+nbt['manaToGet']
        </line>
    </tileentity>
    <tileentity id="botania:enchanter">
        <line displayname="hud.msg.Botania.Mana">
            return nbt['mana']+GRAY+" / "+WHITE+nbt['manaRequired']
        </line>
    </tileentity>
    <tileentity id="botania:specialFlower">
        <line displayname="hud.msg.Botania.Mana">
            return nbt['subTileCmp']['mana']
        </line>
    </tileentity>
    <tileentity id="botania:rfGenerator">
        <line displayname="hud.msg.Botania.Mana">
            return nbt['mana']+GRAY+" / "+WHITE+"1280 ("+nbt['mana']*10+" RF)"
        </line>
    </tileentity>
    <tileentity id="botania:spawnerClaw">
        <line displayname="hud.msg.Botania.Mana">
            return nbt['mana']+GRAY+" / "+WHITE+"160"
        </line>
    </tileentity>
	<entity id="Botania.botania:poolMinecart">
        <line displayname="hud.msg.Botania.Mana">
            return nbt['mana']+GRAY+" / "+WHITE+"1000000"
        </line>
    </entity>
</oo>
