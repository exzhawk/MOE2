<oo>
	<init>
		function gttdFluid(td){
			if(nbt['FluidName']!=undefined)return fluidName(td['FluidName'])+" : "+td['Amount']+" / "+(td['ConnFluid']['Amount'])+" mB"
		}
	</init>
	<tileentity id="thermaldynamics.FluidDuct.*">
		<line>
            gttdFluid(nbt)
		</line>
	</tileentity>
</oo>
